{
  "functions": {
    "src/app/api/workouts/user-detail/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/workouts/*/route.ts": {
      "maxDuration": 60
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, max-age=0"
        }
      ]
    }
  ]
}
